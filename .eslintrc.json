{
   "env": {
     "node": true,
     "browser": true,
     "es6": true
     },
     "parser": "babel-eslint",
     "extends": "airbnb",
     "rules": {
       "react/jsx-filename-extension": [ 1, {
         "extensions": [ ".js", ".jsx" ] } ],
         "react/prefer-stateless-function": 0,
         "no-use-before-define": 0
     }
  }
